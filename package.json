{
  "name": "rubikscube",
  "version": "0.0.1",
  "description": "simulating Rubik's Cube in JavaScript",
  "main": "dist/cube.js",
  "scripts": {
    "preversion": "npm test",
    "version": "npm run build && gulp && git add -A",
    "postversion": "git push && git push --tags",
    "pretest": "npm run build",
    "test": "mocha -R spec",
    "build": "rollup src/cube.js -c -o dist/cube.js --name Cube"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandanoir/rubikscube.git"
  },
  "author": "Naoto Ikuno <moon_in_france@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandanoir/rubikscube/issues"
  },
  "homepage": "https://github.com/pandanoir/rubikscube#readme",
  "devDependencies": {
    "mocha": "3.4.2",
    "rollup": "0.45.1"
  }
}
